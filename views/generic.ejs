<html>
	<head>
		<title>Advocate</title>
		<link rel="stylesheet" href="/public/stylesheets/genericstyle.css" title="Advocate Styling" type="text/css" media="screen" charset="utf-8">
		<script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
		<script src="/public/javascripts/jquery.scrollTo-1.4.3.1-min.js" charset="utf-8"></script>
		<script>
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
			(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
			m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','//www.google-analytics.com/analytics.js','ga');
			
			ga('create', 'UA-52390719-1', 'auto');
			ga('send', 'pageview');
		</script>
		<script src="/node_modules/socket.io/node_modules/socket.io-client/dist/socket.io.js"></script>
	</head>
	<body>
		<div class="overlay" onclick="toggleDialogue()">
			<div class="popup" onclick="preventHide()">
				<h1 class="headTitle centerAlign">A MODAL WINDOW</h1>
				<div class="colorBar">
					<div class="pink colorBlock">
					</div>
					<div class="blue colorBlock">
					</div>
					<div class="aqua colorBlock">
					</div>
					<div class="yellow colorBlock">
					</div>
				</div>		
				<p>Some content is here</p>
			</div>
		</div>
		<div id="wrapper">
			<div id="nav" class="pink">
				<div class="gridWrapper height100 tableView">
					<div id="logo">
						<img src="resources/logo-web.png" />
					</div>
					<div id="navLeft" class="navText">
						<a href="/">Volunteer</a> | <a href="/login">Organize</a>
					</div>
					<div class="navWrapper">
						<div id="navRight">
							<%if(!user){%>
								<a href="/login" class="navText" style="margin-right: 24px;">Log In</a>
								<a href="/register" class="smallButtonize" style="font-size: 14pt;">Sign Up!</a>
							<%}%>
							<%if(user){%>
								<div id="navProfilePic" style="background-image: url(<%=user.profile%>); background-size: cover;">
								</div>
								<div id="navName">
									<p class="navText"><%=user.firstname%> <%=user.lastname%></p>
								</div>
								<div id="navSettings">
								</div>
								<div id="navNotifications">
								</div>
							<%}%>
						</div>
					</div>
				</div>
			</div>
			<div class="colorBar">
				<div class="pink colorBlock">
				</div>
				<div class="blue colorBlock">
				</div>
				<div class="aqua colorBlock">
				</div>
				<div class="yellow colorBlock">
				</div>
			</div>
			<div id="content" class="gridWrapper">
				<h1>This is Header 1</h1>
				<h2>This is Header 2</h2>
				<h3>This is Header 3</h3>
				<p>This is a paragraph, or body text.</p>
				<a href="/">This is a hyperlink.</a>
				<br>
				<br>
				<id class="pink buttonize" onclick="toggleDialogue();">Button</id>
				<br>
				<br>
				<br>
			</div>
			<div id="footer" class="pink banner">
				<div class="gridWrapper">
					<div class="logo">
						<p class="footerText">2014 ADVOCATE. ALL RIGHTS RESERVED</p>
					</div>
				</div>
			</div>
		</div>
		<script src="/public/javascripts/js_generic.js"></script>
	</body>
</html>
